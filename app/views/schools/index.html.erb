<% if user_signed_in? %>
 <div class="wrapper">
   <div class="index_header">
     <h3>Schools</h3>
   </div>
    <% @schools.each do |f| %>
      <a href= <%= position_path(f.id) %> class='more_links' >
        <h4><%= f.name %> - <%= f.city %>, <%= f.state %></h4>
      </a>
    <% end %>
  </div>
<% end %>
