<%= form_for @level do |f| %>
  <% if @level.errors.any? %>
    <div>
      <h2><%= pluralize(@level.errors.count, "error") %> prohibited this note from being saved:</h2>
      <ul>
        <% @level.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  <div class='wrapper'>
    <div class="row">
      <h1 class="form_header">Update Level</h1>
    </div>
    <div class="row">
      <%= f.label :name, 'Name', class: 'form-actions' %>
      <%= f.text_field :name %>
    </div>
    <div class="form-actions">
      <%= f.submit 'Update' %>
      <p><%= link_to "Delete Level", level_path(@level.id), data: { confirm: "Are you sure? Level item should only be deleted if it is a duplicate" }, method: :delete, :class => 'user_links' %> <br />
    </div>
  </div>
<% end %>
