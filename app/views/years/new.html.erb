<div class='wrapper'>
  <%= form_for @year do |f| %>
    <div class="row">
      <h1 class="form_header">Add a New Class</h1>
    </div>
    <div class="row">
      <%= f.label :year, 'Year', class: 'form-actions' %>
      <%= f.text_field :year, class: 'text_field' %>
    </div>
    <div class="row">
      <%= f.label :active, 'Active', class: 'form-actions' %>
      <%= f.select :status, options_for_select([['Active', 'active'], ['Inactive', 'inactive']]) %>
    </div>
    <div class="form-actions">
      <%= f.button 'Submit' %>
    </div>
  <% end %>
</div>
